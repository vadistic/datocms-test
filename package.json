{
  "name": "generic-portfolio-starter",
  "description": "Gatsby portfolio demo",
  "version": "0.1.0",
  "author": "Jakub Wadas <vadistic@gmail.com>",
  "keywords": [
    "gatsby",
    "react",
    "datocms",
    "portfolio",
    "typescript"
  ],
  "license": "MIT",
  "main": "n/a",
  "repository": {
    "type": "git",
    "url": "https://github.com/vadistic/generic-portfolio-starter"
  },
  "scripts": {
    "build": "gatsby build",
    "dev": "gatsby develop",
    "format:all": "prettier --write -l --no-color './{src,types}/**/*.{js,ts,tsx}'",
    "format": "pretty-quick --staged",
    "lint": "tslint -p tsconfig.json",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "env": "node ./scripts/env-config",
    "apollo:check": "apollo service:check --endpoint http://localhost:8000/___graphql",
    "apollo:push": "apollo service:push --endpoint http://localhost:8000/___graphql",
    "apollo:schema": "apollo service:download --endpoint 'http://localhost:8000/___graphql' src/generated/schema.json",
    "apollo:codegen": "apollo client:codegen ./src/generated/graphql.ts --outputFlat --target typescript"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn format && yarn lint && yarn typecheck"
    }
  },
  "dependencies": {
    "babel-plugin-styled-components": "^1.10.0",
    "dotenv": "^6.2.0",
    "gatsby": "^2.0.118",
    "gatsby-image": "^2.0.29",
    "gatsby-plugin-layout": "^1.0.12",
    "gatsby-plugin-react-helmet": "^3.0.6",
    "gatsby-plugin-styled-components": "^3.0.5",
    "gatsby-plugin-typescript": "^2.0.7",
    "gatsby-source-datocms": "^2.0.1",
    "gatsby-source-filesystem": "^2.0.20",
    "gatsby-transformer-remark": "^2.2.4",
    "graphql": "^14.1.1",
    "grommet": "^2.4.0",
    "grommet-icons": "^4.2.0",
    "react": "^16.8.1",
    "react-dom": "^16.8.1",
    "react-helmet": "^5.2.0",
    "react-photo-gallery": "^6.3.1",
    "react-spring": "^8.0.5",
    "react-with-gesture": "^4.0.2",
    "resize-observer-polyfill": "^1.5.1",
    "styled-components": "^4.1.3",
    "typeface-noto-sans": "^0.0.54",
    "typeface-noto-serif": "^0.0.54"
  },
  "devDependencies": {
    "@types/grommet-icons": "vadistic/types#types-grommet-icons-v4.1.0-2-gitpkg",
    "@types/react": "^16.8.2",
    "@types/react-dom": "^16.8.0",
    "@types/react-photo-gallery": "vadistic/types#types-react-photo-gallery-v6.3.0-0-gitpkg",
    "@types/styled-components": "^4.1.8",
    "apollo": "^2.4.4",
    "husky": "^1.3.1",
    "prettier": "^1.16.4",
    "pretty-quick": "^1.10.0",
    "tslint": "^5.12.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-react": "^3.6.0",
    "typescript": "^3.3.3"
  },
  "resolutions": {
    "graphql": "14.1.1"
  }
}
